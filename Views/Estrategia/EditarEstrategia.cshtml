@model Estrategia

@{
    ViewData["Title"] = "EditarEstrategia";
}

<div class="row">

    <div class="col-lg-12">

        <div class="ibox float-e-margins">

            <form asp-controller="Estrategia" asp-action="EditarEstrategia" method="post" class="form-horizontal" role="form">

                <div class="ibox-title">

                    <h5>Editar los datos del Indicador Estratégico <small>Información Anual.</small></h5>

                </div>

                <div class="ibox-content">

                    <div class="form-group">
                        <label class="col-sm-2 control-label">Anio</label>

                        <div class="col-sm-4">

                            @Html.TextBoxFor(model => model.anio, new { @class = "form-control", @type = "number" })

                        </div>
                    </div>

                    <div class="hr-line-dashed"></div>

                    <div class="form-group">

                        <label class="col-sm-2 control-label">Estrategia</label>

                        <div class="col-sm-10">

                            @Html.TextAreaFor(model => model.indicadorEstrategico, new { @class = "form-control", @rows = 3 })
                            <span asp-validation-for="indicadorEstrategico" class="badge badge-danger"></span>
                        </div>

                    </div>

                    <div class="hr-line-dashed"></div>

                    <div class="form-group">

                        <div class="row">

                            <div class="col-lg-6">

                                <div class="ibox-title">

                                    <h5>Programados </h5>

                                </div>

                                <div class="ibox-content">

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Enero </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.ene_p, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Febrero </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.feb_p, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Marzo </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.mar_p, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Abril </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.abr_p, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Mayo </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.may_p, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Junio </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.jun_p, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Julio </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.jul_p, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Agosto </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.ago_p, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Septiembre </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.sep_p, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Octubre </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.oct_p, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Noviembre </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.nov_p, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Diciembe </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.dic_p, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                </div>

                            </div>

                            <div class="col-lg-6">

                                <div class="ibox-title">

                                    <h5>Avances </h5>

                                </div>

                                <div class="ibox-content">

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Enero </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.ene_a, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Febrero </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.feb_a, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Marzo </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.mar_a, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Abril </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.abr_a, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Mayo </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.may_a, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Junio </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.jun_a, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Julio </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.jul_a, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Agosto </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.ago_a, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Septiembre </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.sep_a, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Octubre </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.oct_a, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Noviembre </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.nov_a, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                    <div class="form-group">

                                        <label class="col-sm-2 control-label">Diciembe </label>

                                        <div class="col-sm-4">

                                            @Html.TextBoxFor(model => model.dic_a, new { @class = "form-control", @type = "number" })

                                        </div>

                                    </div>

                                </div>

                            </div>

                        </div>

                    </div>

                <div class="ibox-footer">

                    @Html.HiddenFor(model => model.id)

                    <button class="btn btn-outline btn-success dim" type="submit"><i class="fa fa-floppy-o"></i></button>

                    <button class="btn btn-outline btn-danger dim" type="button" onclick="salir();"><i class="fa fa-sign-out"></i></button>

                </div>

            </form>

        </div>

    </div>

</div>

@section scripts
{


<script language="javascript">

        function salir() {
            location.href = '@Url.Action("Index", "Estrategia")';
        }

</script>

}